      "cell_type": "code",
      "source": [
        "#aplicamos el col transformer\n",
        "\n",
        "x = df.drop(columns=['label'])\n",
        "y = df['label'] # sacamos la var a predecir\n",
        "df_transformed = col_transformer.fit_transform(x)"
